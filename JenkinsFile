node{
     stage ("checkout repo"){
            git branch: 'master'
            url: 'https://github.com/Andrii-Isa/selenoid_example.git'
     }

     stage ("build") {
         steps {
            sh "./gradlew clean assemble"
         }
     }

     stage ("run Selenoid tests"){
         steps {
             sh "./gradlew test"
         }
     }
 }